{
  "name": "morrain-vue-webpack",
  "version": "1.0.0",
  "description": "learning how to use webpack ",
  "private": true,
  "scripts": {
    "dev": "node build/dev-server.js",
    "debug:dev": "node --inspect-brk=5859 build/dev-server.js",
    "build": "node build/build.js",
    "debug:build": "node --inspect-brk=5858 build/build.js",
    "build:test": "node build/build.js --env test",
    "build:release": "node build/build.js --env release",
    "build:online": "node build/build.js --env online",
    "start": "webpack --config webpack.prod.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/morrain/morrain-vue-webpack.git"
  },
  "keywords": [
    "webpack",
    "vue"
  ],
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "author": "626100676@qq.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/morrain/morrain-vue-webpack/issues"
  },
  "homepage": "https://github.com/morrain/morrain-vue-webpack#readme",
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "autoprefixer": "^9.5.1",
    "babel-loader": "^8.0.6",
    "chalk": "^2.4.2",
    "clean-webpack-plugin": "^2.0.2",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.9.0",
